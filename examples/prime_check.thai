ให้ is_prime = ฟังก์ชัน(n) {
    ถ้า (n < 2) {
        คืนค่า เท็จ;
    } ไม่งั้น {
        ให้ i = 2;
        ขณะที่ (i * i < n + 1) {
            ถ้า (n % i == 0) {
                คืนค่า เท็จ;
            } ไม่งั้น {
                ให้ i = i + 1;
            }
        }
        คืนค่า จริง;
    }
};

แสดง("Prime numbers up to 30:");
ให้ num = 2;
ขณะที่ (num < 30) {
    ถ้า (is_prime(num)) {
        แสดง(num);
    } ไม่งั้น {
        ให้ temp = 0;
    }
    ให้ num = num + 1;
}
